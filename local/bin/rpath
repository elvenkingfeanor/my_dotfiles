#!/bin/sh
#rpath
/usr/bin/find /etc /usr /opt /tmp /var -type d \( -path /etc/pacman.d/gnupg -o -path /etc/credstore -o -path /etc/credstore.encrypted -o -path /etc/audit/plugins.d -o -path /usr/share/polkit-1 -o -path /var/lib/NetworkManager -o -path /var/lib/transmission -o -path /var/lib/private -o -path /var/lib/private -o -path /var/lib/machines -o -path /var/lib/iwd -o -path /var/lib/tor -o -path /var/lib/portables -o -path /var/log/private -o -path /var/log/audit -o -path /var/db/sudo -o -path /var/tmp -o -path /var/cache/private -o -path /var/cache/ldconfig -o -path /var/cache/cups -o -path "/tmp/systemd-private-*" \) -prune -false -type f -path /etc/credstore.credentials -prune -false -o -type f -name '*' -print | /usr/bin/fzf | /usr/bin/tr -d '\n' | /usr/bin/xclip -selection clipboard 
