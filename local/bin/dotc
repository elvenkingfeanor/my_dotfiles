#!/bin/sh
#dotc
/usr/bin/find $XDG_CONFIG_HOME/dunst/dunstrc | /usr/bin/xargs -I {} /usr/bin/cp -av {} $HOME/gits/my_dotfiles/dunst/ && /usr/bin/find $XDG_CONFIG_HOME/mpv/ -maxdepth 1 -type f -iname '*.conf' | /usr/bin/xargs -I {} /usr/bin/cp -av {} $HOME/gits/my_dotfiles/mpv/ && /usr/bin/find $XDG_CONFIG_HOME/newsboat/ -type f -not -path '*.bak' -iname '*' | /usr/bin/xargs -I {} /usr/bin/cp -av {} $HOME/gits/my_dotfiles/newsboat/ && /usr/bin/find $XDG_CONFIG_HOME/nsxiv/exec/ -type f | /usr/bin/xargs -I {} /usr/bin/cp -av {} $HOME/gits/my_dotfiles/nsxiv/exec/ && /usr/bin/find $XDG_CONFIG_HOME/youtube-viewer/youtube-viewer.conf | /usr/bin/xargs -I {} /usr/bin/cp -av {} $HOME/gits/my_dotfiles/youtube-viewer/ && /usr/bin/find $XDG_CONFIG_HOME/yt-dlp/yt-dlp.conf | /usr/bin/xargs -I {} /usr/bin/cp -av {} $HOME/gits/my_dotfiles/yt-dlp/ && /usr/bin/find $XDG_CONFIG_HOME/zathura/zathurarc | /usr/bin/xargs -I {} /usr/bin/cp -av {} $HOME/gits/my_dotfiles/zathura/
