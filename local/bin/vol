#!/bin/sh
#vol
#/usr/bin/pamixer -u --set-volume "$1"
if [[ $(/usr/bin/wpctl get-volume @DEFAULT_AUDIO_SINK@ | /usr/bin/awk '{print $NF}') == '[MUTED]' ]]; then
	/usr/bin/echo "off"; else
	/usr/bin/echo "$(/usr/bin/wpctl get-volume @DEFAULT_AUDIO_SINK@ | /usr/bin/awk '{print $NF}')*100" | /usr/bin/bc | /usr/bin/sed 's/\..*$//'
fi
